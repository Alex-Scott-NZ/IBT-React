{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Next.js: Launch Chrome",
        "type": "chrome",
        "request": "launch",
        "url": "http://localhost:3000",
        "webRoot": "${workspaceFolder}",
        "sourceMaps": true,
        "runtimeArgs": ["--incognito"],
        "preLaunchTask": "npm: dev"
      }
    ]
  }
  